{
  "name": "coreytegeler.github.io",
  "version": "1.1.0",
  "description": "a website",
  "main": "jquery.js",
  "scripts": {
    "sass": "sass --watch assets/sass/style.scss:assets/css/style.css",
    "prefix": "postcss assets/css/style.css --use autoprefixer -r assets/css/style.css",
    "pug": "pug --watch ./*.pug --out ./",
    "watch": "npm run sass & npm run prefix",
    "start": "npm run watch & /usr/local/bin/php -S localhost:8000"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coreytegeler/coreytegeler.github.io.git"
  },
  "author": "Corey Tegeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coreytegeler/coreytegeler.github.io/issues"
  },
  "homepage": "https://github.com/coreytegeler/coreytegeler.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "cssnano": "^4.1.10",
    "node-sass": "3.8.0",
    "postcss": "^7.0.18",
    "pug": "^2.0.4",
    "sass": "^1.23.0"
  },
  "dependencies": {
    "postcss-cli": "^6.1.3",
    "pug-cli": "^1.0.0-alpha6"
  }
}
